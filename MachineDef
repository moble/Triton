C++		= g++
C++LINK		= g++
MAKEDEPEND	= g++ -MM
SHARED		= -dynamiclib -undefined dynamic_lookup -single_module
#SHARED		= -shared


AR		= ar
ARFLAGS		= cr
RM		= /bin/rm -f
ECHO		= echo
LN		= ln -sf
SED		= sed
DIFF		= diff

